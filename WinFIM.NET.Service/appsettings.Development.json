{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "serilog:minimum-level": "Information",
  "serilog:using:Console": "Serilog.Sinks.Console",
  "serilog:using:File": "Serilog.Sinks.File",
  "serilog:write-to:File.path": "c:\\tools\\WinFIM.NET\\.log",
  "serilog:write-to:File.shared": true,
  "serilog:write-to:File.rollingInterval": "Day",
  "serilog:write-to:File.formatter": "Serilog.Formatting.Json.JsonFormatter"
}