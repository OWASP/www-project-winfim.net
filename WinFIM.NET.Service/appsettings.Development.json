{
  "Serilog": {
    "MinimumLevel": "Information", // Serilog minimum-level available settings: Verbose, Debug, Information, Warning, Error, Fatal
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          //"outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {Message}{NewLine}{Exception}" //output the console to text with linebreaks
          "formatter": "Serilog.Formatting.Json.JsonFormatter" //output the console to JSON
        }
      }
    ]
  },
  "ConfigurationOptions": {
    "IsLogToWindowsEventLog": false,
    "IsCaptureRemoteConnectionStatus": false,
    "Timer": "0" // What frequency the application will run. A value of 1 = 1 minute, 1s = 1 second, 1m = 1 minute, 1h = 1 hour, 1d = 1 day
  }
  }